              "let bodies = [\"adam\",\"eve\",\"veli\"];\r",
              "let i = pm.collectionVariables.get(\"i\") || 0;\r",
              "\r",
              "pm.collectionVariables.set(\"body\", bodies[i]);\r",
              "\r",
              "i = Number(i) + 1;\r",
              "pm.collectionVariables.set(\"i\", i < bodies.length ? i : 0);\r",
              "\r",
              "if(i < bodies.length) {\r",
              "    pm.execution.setNextRequest(\"Scripted Data – Create Post\");\r",
              "} else {\r",
