          "listen": "test",
          "script": {
            "exec": [
              "pm.test(\"Status code is 200\", function () {\r",
              "    pm.response.to.have.status(200);\r",
              "});\r",
              "\r",
              "const response = pm.response.json();\r",
              "pm.test(\"Board ID found and variable declared\", ()=> {\r",
              "pm.expect(response).to.haveOwnProperty(\"id\");\r",
              "pm.expect(response.id).to.be.a(\"string\");\r",
              "pm.collectionVariables.set(\"boardID\", response.id);\r",
              "\r",
              "})\r",
              "\r",
              "const response2 = pm.response.json();\r",
              "const collectionName = pm.collectionVariables.get(\"Board\" + pm.collectionVariables.get(\"boardNumber\"));\r",
              "pm.test(\"Board name is Test and validated\", ()=> {\r",
              "pm.expect(response2).to.haveOwnProperty(\"name\");\r",
              "pm.expect(response2.name).to.be.a(\"string\");\r",
              "pm.expect(response2.name).eql.toString(collectionName);\r",
              "\r",
              "})"
            ],
            "type": "text/javascript",
            "packages": {},
            "requests": {}
          }
        },
        {
          "listen": "prerequest",
          "script": {
            "exec": [
              "let boardNumber = pm.collectionVariables.get(\"boardNumber\");\r",
              "if (isNaN(boardNumber)) {\r",
              "    boardNumber = 0;\r",
              "}\r",
              "boardNumber++;\r",
              "pm.collectionVariables.set(\"boardNumber\", boardNumber);\r",
              "\r",
              ""
            ],
            "type": "text/javascript",
            "packages": {},
            "requests": {}
          }
